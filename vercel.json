{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "25mb",
        "runtime": "python3.9",
        "includeFiles": ["api/**", "requirements.txt"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "wsgi.py"
    }
  ]
}
